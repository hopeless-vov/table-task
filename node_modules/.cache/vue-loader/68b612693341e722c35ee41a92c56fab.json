    </div>\n  </div>\n</template>\n\n<script>\n  export default {\n    name: 'Table',\n    components: {\n    },\n    props: {\n      headers: {\n        type: Array,\n        default: () => [],\n      },\n      items: {\n        type: Array,\n        default: () => [],\n      },\n      limit: {\n        type: Number,\n        default: () => 10,\n      },\n    },\n    data() {\n      return {\n        page: 1,\n        pageCount: 0,\n      }\n    },\n    methods: {\n      numberFormat(number) {\n        const formated = new Intl.NumberFormat().format(number);\n        return formated;\n      },\n      getFileLength(files) {\n        return files?.length;\n      },\n      showDetails(row) {\n        this.$emit('tableInfo', row);\n      },\n      timeFormat(time) {\n        return time ? time.split('T')[0] : '';\n      },\n    },\n  }\n</script>\n<style lang=\"scss\">\n  .v-data-table {\n    &__wrapper {\n      tr {\n        cursor: pointer;\n      }\n    }\n  }\n</style>\n"]}]}